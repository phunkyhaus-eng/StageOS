# Staging GitHub environment secrets template (Render profile)
# Copy to: deploy/secrets/staging.secrets.local
# Then fill all REPLACE_ME values.

DEPLOY_TARGET=render

DATABASE_URL=postgresql://stageos:REPLACE_ME@staging-db-host:5432/stageos?schema=public
RENDER_API_DEPLOY_HOOK_URL=https://api.render.com/deploy/srv-REPLACE_ME?key=REPLACE_ME
RENDER_WEB_DEPLOY_HOOK_URL=https://api.render.com/deploy/srv-REPLACE_ME?key=REPLACE_ME
RENDER_WORKER_DEPLOY_HOOK_URL=https://api.render.com/deploy/srv-REPLACE_ME?key=REPLACE_ME

STAGING_WEB_BASE_URL=https://staging.example.com
STAGING_API_BASE_URL=https://api-staging.example.com
